(define-primitive-concept Temporalpraep EWN-TEMPORAL-PROPERTY)
(define-primitive-concept Zeitpunktpraep Temporalpraep)

;; Temporale Praepositionen bezeichnen Zeitpunkt, -raum oder -dauer.
;;
;; Kennung: T... [temporal]
;; Zeitpunktpraep: die Praepositionen zur Angabe eines Zeitpunktes.
;;
;; Kennung: TP [temporal: point]

(define-primitive-concept Zeitpunktpraep-P Zeitpunktpraep)
(define-primitive-concept Zeitpunktpraep-B Zeitpunktpraep)
(define-primitive-concept Zeitpunktpraep-R Zeitpunktpraep)
(define-concept
  lc-timepoint
  (or EWN-Day4 EWN-Month2CalendarMonth1))

; Einheit einer Zeitdauer [Minuten, Stunden, etc.]

(define-primitive-concept
  lc-time-unit
  EWN-UnitOfTime1TimeUnit1)

; Konzepte fr Prpositionen
; Richtung vor/nach
; [define-primitive-concept direction TimeExpr]
; Zeitpunkt um/an/in
; [define-primitive-concept temporal-location TimeExpr]
; Intervallgrenze von/ab/bis
; [define-primitive-concept limit TimeExpr]

; time-part-adj = fruehen/spaeten
(define-concept
  lc-time-part-adj
  (or EWN-Late1
      EWN-Early1))

; direction-adv = frueher/spaeter
(define-concept lc-direction-adv lc-time-part-adj)

(define-primitive-attribute year-value)
(define-primitive-attribute hour-value)
(define-primitive-attribute day-value)
(define-primitive-attribute month-value)
(define-primitive-attribute minute-value)

; Zahlen im Zeitkontext [hier knnten dann Angaben zu Maximalwerten
;                        hinzugefgt werden].

(define-concept day (and EWN-Day4 (all day-value EWN-Number1)))
(define-concept hour (and EWN-60Minutes1Hour1Hr1
			  (all hour-value EWN-Number1)))
(define-concept minute (and EWN-Min1Minute1
			 (all minute-value EWN-Number1)))
(define-concept month (and EWN-Month2CalendarMonth1 
			   (all month-value EWN-Number1)))
(define-concept year (and EWN-Yr1TwelveMonth1Year2
			  (all year-value EWN-Number1)))

(define-primitive-attribute has-attribute)
(define-primitive-attribute has-unit)

(define-primitive-attribute has-hour)
(define-primitive-attribute has-minutes)
(define-primitive-attribute has-basetime)
(define-primitive-attribute has-timedistance)
(define-primitive-attribute has-baseduration)
(define-primitive-attribute has-direction)
(define-primitive-attribute is-fuzzy)

(define-primitive-attribute has-start-hour)
(define-primitive-attribute has-middle-hour)
(define-primitive-attribute has-end-hour)
(define-primitive-attribute has-traditional-hour)
(define-primitive-attribute has-month)
(define-primitive-attribute has-day)
(define-primitive-attribute has-year)
(define-primitive-attribute has-dow)

(define-primitive-attribute has-date)
(define-primitive-attribute has-timepoint)

(define-primitive-concept lc-clocktime-or-pod lc-timepoint)
(define-primitive-concept lc-date-or-dow lc-timepoint)

(disjoint lc-clocktime-or-pod lc-date-or-dow)

(define-primitive-concept lc-now lc-clocktime-or-pod)

(define-concept lc-clocktime (and lc-clocktime-or-pod
			          (all has-hour hour)
				  (all has-minutes minute)))

; "normale Uhrzeit" 
(define-concept lc-traditional-clocktime (and lc-clocktime
					      (all has-traditional-hour hour)))

; fr Angaben wie "viertel vor" etc.
(define-concept lc-special-minutes
  (and lc-clocktime
       (all has-basetime lc-clocktime)
       (all has-direction cn)))

(define-concept lc-special-minutes-from
  (and lc-special-minutes
       (all has-timedistance minute)))

; fr Angabe "Punkt xy Uhr" 
(define-primitive-concept lc-punkt lc-clocktime)

; Tageszeiten
(define-concept lc-part-of-day (and lc-clocktime-or-pod
				    (all has-start-hour cn)
			            (all has-end-hour cn)
				    (all has-middle-hour cn)))

(define-primitive-concept lc-regular-part-of-day lc-part-of-day)
(define-primitive-concept lc-irregular-part-of-day lc-part-of-day)

(disjoint lc-regular-part-of-day lc-irregular-part-of-day)
; fr: frher/spter Abend u..
(define-concept lc-part-of-part-of-day (and lc-regular-part-of-day
	     			         (all has-attribute lc-time-part-adj)))

; Wochentag
(define-primitive-concept lc-dow lc-date-or-dow)

; Jahr und Tag werden immer als Zahl angegeben, der Monat dagegen
; auch als Monatsname, daher ist nur der Monat ein eigenes Konzept
(define-concept lc-date (and lc-date-or-dow
			     (all has-year year)
			     (all has-month month)
			     (all has-day day)))

(define-concept TimeDate (and lc-time-unit
			      (all has-date lc-date-or-dow)
			      (all has-timepoint lc-clocktime-or-pod)))

; Feiertag
(define-primitive-concept lc-holiday lc-date)
(define-primitive-concept NewYear lc-holiday)

; heute/gestern/morgen ...
