; [define-concept ac-deictic-date [and ac-date 
;  				     [all has-lexconcept lc-deictic-date]]]

(define-concept lc-days-from-now (and lc-date
				      (all has-timedistance day)
				      (all has-direction cn))) ;one-of vor nach

; fr Pfingsten etc.
(define-concept lc-days-from-easter (and lc-date
				      (all has-timedistance day)
				      (all has-direction cn))) ;one-of vor nach


; Kombination aus Wochentag und Datum [z.B. Montag der dritte]
(define-concept lc-dowdate (and lc-date
	                	(all has-dow lc-dow)))

; fr dieser/nchster/letzter
(define-concept lc-tnl (and EWN-TEMPORAL-PROPERTY
			   (all has-timedistance day)
		           (all has-direction cn))) ;one-of vor nach

; fr nchsten Dienstag
(define-concept lc-dow-from-now (and lc-tnl
			   (all has-dow lc-dow)))

; Zeitdauer mit Einheit und Wert. Wert kann entweder eine Kardinalzahl
; oder viertel/halb/dreiviertel/ganz sein
(define-concept lc-duration (and EWN-TEMPORAL-PROPERTY
			      (all has-unit lc-time-unit)
